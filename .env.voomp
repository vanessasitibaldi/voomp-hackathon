# Configurações dos Serviços Voomp
# Copie este arquivo para .env se quiser customizar as configurações

# PostgreSQL N8N
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n
POSTGRES_DB=n8n
POSTGRES_PORT=5432

# PostgreSQL Evolution
EVOLUTION_POSTGRES_USER=evolution
EVOLUTION_POSTGRES_PASSWORD=evolution123
EVOLUTION_POSTGRES_DB=evolution
EVOLUTION_POSTGRES_PORT=5433

# N8N
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678
GENERIC_TIMEZONE=America/Sao_Paulo

# Evolution API
EVOLUTION_PORT=8080
AUTHENTICATION_API_KEY=voomp-local-key
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
REDIS_ENABLED=true
REDIS_PREFIX=evolution

# Redis
REDIS_PORT=6379