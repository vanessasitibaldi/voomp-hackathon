{
  "name": "voomp-whatsapp-monitor",
  "version": "1.0.0",
  "description": "Sistema de recuperação automática de vendas para Voomp",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.server.json && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:frontend": "vite build",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server.ts",
    "dev:frontend": "vite",
    "watch": "tsc -p tsconfig.server.json --watch",
    "docker:start": "./voomp.sh start",
    "docker:stop": "./voomp.sh stop",
    "docker:restart": "./voomp.sh restart",
    "docker:status": "./voomp.sh status",
    "docker:monitor": "./voomp.sh monitor",
    "docker:monitor:live": "./voomp.sh monitor --continuous",
    "docker:monitor:full": "./voomp.sh monitor --all",
    "docker:logs:evolution": "./voomp.sh logs evolution",
    "docker:logs:n8n": "./voomp.sh logs n8n",
    "docker:logs:postgres": "./voomp.sh logs postgres",
    "docker:logs:redis": "./voomp.sh logs redis",
    "docker:health": "./voomp.sh health",
    "docker:clean": "./voomp.sh clean",
    "docker:reset": "./voomp.sh reset",
    "docker:update": "./voomp.sh update",
    "docker:shell:evolution": "./voomp.sh shell evolution",
    "docker:shell:n8n": "./voomp.sh shell n8n",
    "docker:debug:evolution": "./voomp.sh debug evolution",
    "docker:debug:n8n": "./voomp.sh debug n8n"
  },
  "keywords": [
    "voomp",
    "whatsapp",
    "n8n",
    "sales",
    "recovery"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "vite": "^5.0.8"
  }
}
